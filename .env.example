# Login Credentials (required)
# Set your own username and password
VITE_LOGIN_USERNAME=your_username
VITE_LOGIN_PASSWORD=your_password

# Google Maps API Key (required)
# Get your API key from: https://console.cloud.google.com/google/maps-apis
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Rate Limiting Configuration
# Set these to control API request behavior

# Distance Matrix API - Minimum milliseconds between requests
VITE_RATE_LIMIT_DISTANCE_MATRIX_MIN_INTERVAL_MS=2000

# Distance Matrix API - Max requests per minute
VITE_RATE_LIMIT_DISTANCE_MATRIX_PER_MINUTE=25

# Distance Matrix API - Max requests per hour
VITE_RATE_LIMIT_DISTANCE_MATRIX_PER_HOUR=100

# Distance Matrix API - Max requests per day
VITE_RATE_LIMIT_DISTANCE_MATRIX_PER_DAY=2500

# Global Daily Quota - Max API requests per day across all endpoints
VITE_GLOBAL_QUOTA_DAILY=1000

# Global Monthly Quota - Max API requests per month across all endpoints
VITE_GLOBAL_QUOTA_MONTHLY=25000

# Cache Configuration - Time-to-live for cached responses (milliseconds)
VITE_CACHE_TTL_MS=300000

# Cache Configuration - Max cached entries per endpoint
VITE_CACHE_MAX_ENTRIES=100

# Enable/Disable caching (true/false)
VITE_CACHE_ENABLED=true

# Exponential Backoff - Initial retry delay (milliseconds)
VITE_BACKOFF_INITIAL_DELAY_MS=1000

# Exponential Backoff - Maximum retry delay (milliseconds)
VITE_BACKOFF_MAX_DELAY_MS=60000

# Exponential Backoff - Multiplier for each retry
VITE_BACKOFF_MULTIPLIER=2

# Enable/Disable exponential backoff (true/false)
VITE_BACKOFF_ENABLED=true

# Max retries for failed requests
VITE_MAX_RETRIES=3

# Show warnings in console when quota limits are approached
VITE_SHOW_QUOTA_WARNINGS=true

# Log all rate limiting events
VITE_LOG_RATE_LIMIT_EVENTS=true
